<div>
    <h1>Welcome To Smell The Roses App!</h1>
    <h3>Sign Up or Sign in Below</h3>
</div>
<%= form_for @owner do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit %>
<% end %>